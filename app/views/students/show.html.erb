<center>
  <h1>Your Profile</h1>
</center>

<center>
  <p>
    <%= @student.name %>
  </p>

  <p>
    <%= @student.bio %>
  </p>

  <% @student.projects.each do |project| %>
    <li>
      <%= project.name %>
      <%= project.url_link %>
    </li>
  <% end %>
  <%= link_to "Edit Student", edit_student_path(@student) %> <br>
  <%= link_to "All students", students_path %>
</center>
